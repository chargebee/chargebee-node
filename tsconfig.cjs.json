{
    "compilerOptions": {
        "outDir": "./cjs",
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es2017",
        "strict": true,
        "types": [
            "node"
        ],
        "esModuleInterop": false,
        "skipLibCheck": true
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "./src/*js",
        "./src/*js",
        "./src/chargebee.esm.ts",
        "./src/chargebee.esm.worker.ts"
    ],
}